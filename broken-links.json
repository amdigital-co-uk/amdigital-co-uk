{
    "ignorePatterns":[
        { "pattern": "^https://" },
        { "pattern": "^http://localhost" },
        { "pattern": "^https://github.com" },
        { "pattern": "^https://docs.github.com" }
    ],
    "replacementPatterns": [
        {
            "pattern": "%2D",
            "replacement": "-"
        },
        {
            "pattern": "%26",
            "replacement": "&"
        },
        {
            "pattern": "/$",
            "replacement": ".md"
        },
        {
            "pattern": "[^.md]$",
            "replacement": "$1.md"
        }
    ]
}